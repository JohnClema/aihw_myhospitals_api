@echo off
REM Cleanup script for aihwmyhospitals R package (Windows)
REM Removes Rust build artifacts after package installation

cd src

REM Remove Rust target directory (compiled artifacts)
if exist rust\target rmdir /s /q rust\target

REM Remove temporary cargo home directory (used during CRAN builds)
if exist .cargo rmdir /s /q .cargo

exit /b 0
